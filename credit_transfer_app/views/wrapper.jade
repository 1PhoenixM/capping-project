        script.
          $(document).ready(function(){
          $(window).resize( function(){
          if ($("body").width() <= 750){
          if ($("#expand-mainnav").length == 0){
          $("#mainnav li").hide();
          $("#mainnav").prepend("<li id='expand-mainnav'><a href='#'><strong>Menu</strong><img src='http://www.marist.edu/currentstudents/images/plusminus.png' style='float:right'></a></li>");
          $("#expand-mainnav").on("click", function(e){
          e.preventDefault();
          if ($("#mainnav li").eq(1).css("display") == "none"){
          $("#mainnav li").not("#expand-mainnav").show();
          }
          else{
          $("#mainnav li").not("#expand-mainnav").hide();
          }
          });
          $("#body-contacts").css({"text-align": "left", "padding-left": "5px", "padding-right": "10px"}).remove().insertAfter("#body-main-content");
          //$("#topbar-search").css("border-width", "0px");
          }
          }
          else{
          $("#body-contacts").css({"padding-left": 0, "padding-right": 0}).remove().insertAfter("#sidebar nav");
          $("#expand-mainnav").remove();
          $("#mainnav li").show();
          //$("#topbar-search").css("border-width", "1px");
          }
          });
          $(window).resize();
          });
